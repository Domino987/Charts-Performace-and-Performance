{
  "extends": ["tslint:recommended", "tslint-react", "tslint-config-prettier"],
  "linterOptions": {
    "exclude": [
      "config/**/*.js",
      "node_modules/**/*.ts"
    ],
    "adjacent-overload-signatures": false,
    "no-shadowed-variable": [
      true,
      {
        "temporalDeadZone": false
      }
    ]
  }
}